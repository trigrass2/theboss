define target/stm32f10x/configure
    TARGET_ARCH:=arm-none-eabi
	TARGET_CFLAGS:=-DSTM32F10X_MD -specs=nosys.specs -mcpu=cortex-m3 -mthumb -mlittle-endian -mfloat-abi=soft -mthumb-interwork
    TARGET_LDADD:=-specs=nosys.specs -mcpu=cortex-m3 -mthumb -mlittle-endian -mfloat-abi=soft -mthumb-interwork
	TARGET_DEPENDS:=
	TARGET_PLATFORM:=stm32f10x
endef


